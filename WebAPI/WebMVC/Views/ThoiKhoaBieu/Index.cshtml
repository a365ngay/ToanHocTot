@model IEnumerable<WebMVC.Models.ThoiKhoaBieu>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    if (Session["Username"] == null)
    {
        Response.Redirect("~/Login/Index");
    }
}

<h2>Thông tin lịch học của học sinh</h2>
<table class="table">
    <tr>
        <th>
            Các buổi học trong tuần
        </th>
        <th>
            Thời gian học
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NgayHoc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ThoiGianHoc)
        </td>
    </tr>
}

</table>
